<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>
	<path>atr833-ii.ac</path>
	
	<params>
		<current-sel type="string">instrumentation/atr833-ii/current-selection</current-sel>
		<cursor-sel type="string">instrumentation/atr833-ii/cursor-selection</cursor-sel>
	</params>
	
	<effect>
		<inherits-from>Effects/model-interior-display</inherits-from>
		<parameters>
			<dirt-factor>1.0</dirt-factor>
			<contrast>0.75</contrast>
		</parameters>
		<object-name>atr833.display</object-name>
	</effect>
	
	<animation>
		<type>material</type>
		<object-name>atr833.display</object-name>
		<emission>
			<red>   0.05 </red>
			<green> 0.05 </green>
			<blue>  0.05 </blue>
			<factor-prop>instrumentation/comm[0]/brightness</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>atr833.vol_sel_knob</object-name>
		<property>instrumentation/atr833-ii/vol-sel-knob</property>
		<axis>
			<x>-1</x>
		</axis>
		<center>
			<y-m> 0.023</y-m>
			<z-m> 0.023</z-m>
		</center>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/atr833-ii/vol-sel-knob</property>
				<factor>1</factor>
				<min>0</min>
				<max>360</max>
				<wrap>1</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/current-sel"/>
						<value>0</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/volume</property>
				<factor>0.05</factor>
				<min>0.0</min>
				<max>1.0</max>
				<wrap>0</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/current-sel"/>
						<value>1</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/squelch</property>
				<factor>1</factor>
				<min>0</min>
				<max>9</max>
				<wrap>0</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/current-sel"/>
						<value>2</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/vox</property>
				<factor>1</factor>
				<min>0</min>
				<max>9</max>
				<wrap>0</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/current-sel"/>
						<value>3</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/intercom-volume</property>
				<factor>1</factor>
				<min>0</min>
				<max>20</max>
				<wrap>0</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/current-sel"/>
						<value>4</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/sidetone-volume[0]</property>
				<factor>1</factor>
				<min>0</min>
				<max>20</max>
				<wrap>0</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/current-sel"/>
						<value>5</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/sidetone-volume[1]</property>
				<factor>1</factor>
				<min>0</min>
				<max>20</max>
				<wrap>0</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/current-sel"/>
						<value>6</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/external-volume</property>
				<factor>1</factor>
				<min>0</min>
				<max>20</max>
				<wrap>0</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/current-sel"/>
						<value>7</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/brightness</property>
				<factor>1</factor>
				<min>0</min>
				<max>9</max>
				<wrap>0</wrap>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>atr833.freq_knob</object-name>
		<property>instrumentation/atr833-ii/freq-knob</property>
		<axis>
			<x>-1</x>
		</axis>
		<center>
			<y-m> 0.023</y-m>
			<z-m>-0.023</z-m>
		</center>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/atr833-ii/freq-knob</property>
				<factor>1</factor>
				<min>0</min>
				<max>360</max>
				<wrap>1</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/cursor-sel"/>
						<value>1</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/frequencies/standby-mhz</property>
				<mask>integer</mask>
				<step>1</step>
				<min>118</min>
				<max>136</max>
				<resolution>1</resolution>
				<wrap>true</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/cursor-sel"/>
						<value>2</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/frequencies/standby-mhz</property>
				<mask>decimal</mask>
				<step>0.1</step>
				<min>0</min>
				<max>1</max>
				<resolution>0.1</resolution>
				<wrap>true</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../../params/cursor-sel"/>
						<value>3</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/frequencies/standby-mhz</property>
				<mask>decimal</mask>
				<step>0.008333</step>
				<min>0</min>
				<max>1</max>
				<resolution>0.008333</resolution>
				<wrap>true</wrap>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>atr833.io_btn</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>atr833_ii.io_btn(1);</script>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/atr833-ii/io-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>nasal</command>
					<script>atr833_ii.io_btn(0);</script>
				</binding>
				<binding>
					<command>property-assign</command>
					<property>instrumentation/atr833-ii/io-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>atr833.io_btn</object-name>
		<property>instrumentation/atr833-ii/io-btn</property>
		<factor>-0.001</factor>
		<axis>
			<x>1</x>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>atr833.set_btn</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/atr833-ii/current-selection</property>
				<step>1</step>
				<min>0</min>
				<max>7.1</max>
				<wrap>1</wrap>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/atr833-ii/set-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>instrumentation/atr833-ii/set-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>atr833.set_btn</object-name>
		<property>instrumentation/atr833-ii/set-btn</property>
		<factor>-0.001</factor>
		<axis>
			<x>1</x>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>atr833.cursor_btn</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/atr833-ii/cursor-selection</property>
				<step>1</step>
				<min>0</min>
				<max>3.1</max>
				<wrap>1</wrap>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/atr833-ii/cursor-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>instrumentation/atr833-ii/cursor-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>atr833.cursor_btn</object-name>
		<property>instrumentation/atr833-ii/cursor-btn</property>
		<factor>-0.001</factor>
		<axis>
			<x>1</x>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>atr833.swap_btn</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-swap</command>
				<property>instrumentation/comm[0]/frequencies/selected-mhz</property>
				<property>instrumentation/comm[0]/frequencies/standby-mhz</property>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/atr833-ii/swap-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>instrumentation/atr833-ii/swap-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>atr833.swap_btn</object-name>
		<property>instrumentation/atr833-ii/swap-btn</property>
		<factor>-0.001</factor>
		<axis>
			<x>1</x>
		</axis>
	</animation>
		

</PropertyList>
