<PropertyList>
    <path>tablet-screen.ac</path>
    <animation>
        <object-name>tablet-screen</object-name>
    </animation>
    <effect>
        <inherits-from>Aircraft/DR400-dauphin/Models/Effects/effects-interior</inherits-from>
        <object-name>tablet-screen</object-name>
    </effect>

    <animation>
        <type>material</type>
        <object-name>tablet-screen</object-name>
        <condition>
            <not>
                <property>sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <red>   0.002 </red>
            <green> 0.002 </green>
            <blue>  0.002 </blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

